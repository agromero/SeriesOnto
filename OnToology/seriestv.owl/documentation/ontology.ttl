@prefix : <http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#> .

<http://www.semanticweb.org/agus/ontologies/2022/2/tvseries> rdf:type owl:Ontology ;
                                                              rdfs:comment "Autor: Agust√≠ Romero Pastor"@en ,
                                                                           "TVSeries ontology"@en ,
                                                                           "This is a Ontology about TV Shows"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#string
xsd:string owl:equivalentClass rdfs:Literal .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#appearsIn
:appearsIn rdf:type owl:ObjectProperty ;
           owl:inverseOf :hasActor ;
           rdf:type owl:TransitiveProperty ;
           rdfs:domain :Character ;
           rdfs:range :Episode .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#contains
:contains rdf:type owl:ObjectProperty ;
          owl:inverseOf :isIncluded ;
          rdf:type owl:TransitiveProperty ;
          rdfs:domain :TVSeries ;
          rdfs:range :Episode .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#defines
:defines rdf:type owl:ObjectProperty ;
         owl:inverseOf :hasGenre ;
         rdf:type owl:InverseFunctionalProperty ;
         rdfs:domain :Genres ;
         rdfs:range :TVSeries .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasActor
:hasActor rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          rdfs:domain :Episode ;
          rdfs:range :Character .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasGenre
:hasGenre rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :TVSeries ;
          rdfs:range :Genres .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#isIncluded
:isIncluded rdf:type owl:ObjectProperty ,
                     owl:TransitiveProperty ;
            rdfs:domain :Episode ;
            rdfs:range :TVSeries .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#isMadeIn
:isMadeIn rdf:type owl:ObjectProperty ;
          owl:inverseOf :isSpokenIn ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :TVSeries ;
          rdfs:range :Language ;
          rdfs:comment "a firm is made in a Language"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#isMadeUpOf
:isMadeUpOf rdf:type owl:ObjectProperty ;
            owl:inverseOf :isMemberOf ;
            rdf:type owl:TransitiveProperty ;
            rdfs:domain :Episode ;
            rdfs:range :EpisodeMembers .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ,
                     owl:TransitiveProperty ;
            rdfs:domain :EpisodeMembers ;
            rdfs:range :Episode ;
            rdfs:comment """an actor/director/producer/writer is member of an episode
a series creator/musician is member of TVSeries"""@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#isPlayedBy
:isPlayedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :plays ;
            rdf:type owl:InverseFunctionalProperty ;
            rdfs:domain :Character ;
            rdfs:range :EpActor .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#isRatedWith
:isRatedWith rdf:type owl:ObjectProperty ,
                      owl:TransitiveProperty ;
             rdfs:domain :Episode ;
             rdfs:range :AdvisoryLabel .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#isSpokenIn
:isSpokenIn rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Language ;
            rdfs:range :TVSeries .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#plays
:plays rdf:type owl:ObjectProperty ,
                owl:FunctionalProperty ;
       rdfs:domain :EpActor ;
       rdfs:range :Character ;
       rdfs:comment "an actor plays a character"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#belongsToSeason
:belongsToSeason rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Episode ;
                 rdfs:range xsd:integer ;
                 rdfs:comment "indicates that an Episode belongs to a concrete Season"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasAgeClassification
:hasAgeClassification rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain :TVSeries ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Indicates TVSeries Age Classification"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasCharacterName
:hasCharacterName rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :Character ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Indicates Name of the character played by an actor"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasCountry
:hasCountry rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :TVSeries ;
            rdfs:range xsd:string ;
            rdfs:comment "Indicates TVSeries Country of origin"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasFilmingLocation
:hasFilmingLocation rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :Episode ,
                                :TVSeries ;
                    rdfs:range xsd:string ;
                    rdfs:comment "indicates the Location where TVSeries or Episode is filmed"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasIMDB_Rating
:hasIMDB_Rating rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Episode ,
                            :TVSeries ;
                rdfs:range xsd:decimal ;
                rdfs:comment "Indicates Episode and TVSeries have a rating on IMDB"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasLabel
:hasLabel rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :AdvisoryLabel ;
          rdfs:range xsd:string ;
          rdfs:comment "Indicates wich labels contains AdvisoryLabel" .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasName
:hasName rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :EpActor ,
                     :EpDirector ,
                     :EpWriter ,
                     :SeriesCreator ,
                     :SeriesMusician ,
                     :SeriesProducer ;
         rdfs:range xsd:string ;
         rdfs:comment "indicates name of Members of cast & crew"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasNumber
:hasNumber rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Episode ;
           rdfs:range xsd:integer ;
           rdfs:comment "Indicates the number of an episode"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasNumberOfSeasons
:hasNumberOfSeasons rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :TVSeries ;
                    rdfs:range xsd:integer ;
                    rdfs:comment "Indicates the numer of seasons of the TVSeries"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasRuntime
:hasRuntime rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Episode ;
            rdfs:range xsd:integer ;
            rdfs:comment "Indicates the duration of an Episode"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasSubgenre
:hasSubgenre rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Genres ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasSuperheroName
:hasSuperheroName rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :Superhero ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Indicates the name of a Superhero in case the character is one of them"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasSynopsis
:hasSynopsis rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Episode ,
                         :TVSeries ;
             rdfs:range xsd:string ;
             rdfs:comment "Indicates a brief summary of the TVSeries or Episode"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasTitle
:hasTitle rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Episode ,
                      :TVSeries ;
          rdfs:range xsd:string ;
          rdfs:comment "Indicates the Title of the TVSeries or Episode"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#hasVariant
:hasVariant rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Language ;
            rdfs:range xsd:string ;
            rdfs:comment "Indicates the Variant of a language"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#isProducedBy
:isProducedBy rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :TVSeries ;
              rdfs:range xsd:string ;
              rdfs:comment "Indicates the companies that produce the series"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#wasReleasedOn
:wasReleasedOn rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Episode ,
                           :TVSeries ;
               rdfs:range xsd:dateTime ;
               rdfs:comment "Indicates the data when TVSeries or Episode was released"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#AdvisoryLabel
:AdvisoryLabel rdf:type owl:Class ;
               rdfs:comment "This class denotes Content advisory system Label"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Animation
:Animation rdf:type owl:Class ;
           rdfs:subClassOf :Genres ;
           rdfs:comment "This class denotes Animation genre"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#CastAndCrew
:CastAndCrew rdf:type owl:Class ;
             rdfs:comment "This class denotes members of the entire TVSeries or some Episodes"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Character
:Character rdf:type owl:Class ;
           rdfs:comment "This class denotes Series Character"@en ;
           rdfs:sameAs "https://dbpedia.org/ontology/FictionalCharacter" .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Comedy
:Comedy rdf:type owl:Class ;
        rdfs:subClassOf :Genres ;
        rdfs:comment "This class denotes Comedy genre"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Documentary
:Documentary rdf:type owl:Class ;
             rdfs:subClassOf :Genres ;
             rdfs:comment "This class denotes Documentary genre"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Drama
:Drama rdf:type owl:Class ;
       rdfs:subClassOf :Genres ;
       rdfs:comment "This class denotes drama genre"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#English
:English rdf:type owl:Class ;
         rdfs:subClassOf :Language ;
         rdfs:comment "This clase denotes English language variants"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#EpActor
:EpActor rdf:type owl:Class ;
         rdfs:subClassOf :EpisodeMembers ;
         rdfs:comment "This class denotes Episode Actors"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#EpDirector
:EpDirector rdf:type owl:Class ;
            rdfs:subClassOf :EpisodeMembers ;
            rdfs:comment "This class denotes Episode Directors"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#EpWriter
:EpWriter rdf:type owl:Class ;
          rdfs:subClassOf :EpisodeMembers ;
          rdfs:comment "This class denotes Episode Writer"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Episode
:Episode rdf:type owl:Class ;
         rdfs:comment "This class denotes Episode"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#EpisodeMembers
:EpisodeMembers rdf:type owl:Class ;
                rdfs:subClassOf :CastAndCrew ;
                rdfs:comment "This class denotes members that work in some Episodes" .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Fantastic
:Fantastic rdf:type owl:Class ;
           rdfs:subClassOf :Genres ;
           rdfs:comment "This class denotes fantastic genre"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Genres
:Genres rdf:type owl:Class ;
        rdfs:comment "This classe denotes the main genres of a TVSeries"@en ;
        rdfs:sameAs "https://dbpedia.org/page/Category:Film_series_by_genre" .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Language
:Language rdf:type owl:Class ;
          rdfs:comment "This class denotes language"@en ;
          rdfs:sameAs "https://dbpedia.org/page/Language" .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#LongSeries
:LongSeries rdf:type owl:Class ;
            rdfs:subClassOf :Series ;
            owl:disjointWith :ShortSeries ;
            rdfs:comment "This class denotes series composed of 3 or more seasons" .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Miniseries
:Miniseries rdf:type owl:Class ;
            rdfs:subClassOf :TVSeries ;
            owl:disjointWith :Series ;
            rdfs:comment "This class denotes the type Miniseries, so TVseries with limited number of episodes, and a definite beginning, middle, and end."@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Pilot
:Pilot rdf:type owl:Class ;
       rdfs:subClassOf :Episode ;
       rdfs:comment "This classe denotes a Pilot Episode"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Reality
:Reality rdf:type owl:Class ;
         rdfs:subClassOf :Genres ;
         rdfs:comment "This class denotes reality genre"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Regular
:Regular rdf:type owl:Class ;
         rdfs:subClassOf :Episode ;
         rdfs:comment "This classe denotes a Regular Episode"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Series
:Series rdf:type owl:Class ;
        rdfs:subClassOf :TVSeries ;
        rdfs:comment "This class denotes the type of series composed of several seasons"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#SeriesCreator
:SeriesCreator rdf:type owl:Class ;
               rdfs:subClassOf :CastAndCrew ;
               rdfs:comment "This class denotes Series Creator"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#SeriesMusician
:SeriesMusician rdf:type owl:Class ;
                rdfs:subClassOf :CastAndCrew ;
                rdfs:comment "This class denotes Series Musician"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#SeriesProducer
:SeriesProducer rdf:type owl:Class ;
                rdfs:subClassOf :CastAndCrew ;
                rdfs:comment "This class denotes Episode Producer"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#ShortSeries
:ShortSeries rdf:type owl:Class ;
             rdfs:subClassOf :Series ;
             rdfs:comment "This class denotes series composed of 1 or 2 seasons" .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Spanish
:Spanish rdf:type owl:Class ;
         rdfs:subClassOf :Language ;
         rdfs:comment "This clase denotes Spanish language variants"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Special
:Special rdf:type owl:Class ;
         rdfs:subClassOf :Episode ;
         rdfs:comment "This classe denotes an Special Episode"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Superhero
:Superhero rdf:type owl:Class ;
           rdfs:subClassOf :Character ;
           rdfs:comment "This class denotes Superhero type of character"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#TVSeries
:TVSeries rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasGenre ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Genres
                          ] ;
          rdfs:comment "This class denotes TVSeries"@en .


###  http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Action/Adventure
<http://www.semanticweb.org/agus/ontologies/2022/2/tvseries#Action/Adventure> rdf:type owl:Class ;
                                                                              rdfs:subClassOf :Genres ;
                                                                              rdfs:comment "This class denotes Action and Adventure genres"@en .


